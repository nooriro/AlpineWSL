export CHARSET=UTF-8
export LANG=C.UTF-8
export PAGER=less
export PS1='\h:\w\$ '
export CC='clang -fuse-ld=lld -rtlib=compiler-rt -unwindlib=libunwind -ffunction-sections -fdata-sections -flto'
export CXX='clang++ -fuse-ld=lld -rtlib=compiler-rt -unwindlib=libunwind -ffunction-sections -fdata-sections -flto -stdlib=libc++ -lc++abi'
umask 022
cd ~

for script in /etc/profile.d/*.sh ; do
        if [ -r $script ] ; then
                . $script
        fi
done
